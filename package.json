{
  "name": "test-youtrack-import-tool",
  "version": "1.0.0",
  "description": "Import and synchronize GitHub issues with YouTrack tasks",
  "main": "dist/index.js",
  "bin": {
    "github-youtrack-sync": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc -w & nodemon dist/index.js",
    "test": "build && node dist/index.js test",
    "import": "npm run build && node dist/index.js import",
    "sync": "npm run build && node dist/index.js sync",
    "webhook": "npm run build && node dist/index.js webhook",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@octokit/webhooks": "^14.1.3",
    "axios": "^1.12.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "octokit": "^5.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "nodemon": "^3.1.10",
    "typescript": "^5.5.3"
  },
  "private": true
}
